package com.ruby.java.ch08;

enum Status {
	READY, SEND, COMPLETE, CLOSE
}

public class EnumTest01 {
	public static void main(String[] args) {
		Status work = null; 
		
		int n = 2; 
		
		switch (n) {
		case 1:
			work= Status.READY;
			break; 
		
		}
		
		System.out.println("Work Status is: " + work);
	}

}
