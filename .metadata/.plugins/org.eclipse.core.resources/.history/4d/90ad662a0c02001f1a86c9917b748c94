package com.ruby.java.ch07;

public class Employee {
	private String name; 
	private int age;
	private String dept;
	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name= name ;
	}
	
	public int getAge() {
		return age; 
	}
	
	public String getDept() {
		return dept;
	}

	public void setDept(String dept) {
		this.dept = dept;
	}
}