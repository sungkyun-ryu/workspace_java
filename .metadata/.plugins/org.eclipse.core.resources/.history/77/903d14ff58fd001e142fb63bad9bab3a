package apr18;

public class Work_4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int arr[][]= new int[3][5];
		for(int i=0; i<arr.length; i++) {
			for(int j=0; j<arr[i].length; j++) {
				arr[i][j]= j + 1 + (5*i); 
			}
		} System.out.println(arr[2][0]);
	} 
}
