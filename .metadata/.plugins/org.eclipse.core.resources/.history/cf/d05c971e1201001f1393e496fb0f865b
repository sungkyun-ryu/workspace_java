package assignment;

public class MatrixCalc {
	
	private int[][] matrix= {
					{1,2,3},
					{4,5,6},
					{7,8,9}
	};
	
	private void work_row() {
		
		for(int i=0; i<matrix.length; i++) {
			for(int j=0; j<matrix[i].length; j++) {
				int sum= 0; 
				sum += matrix[i][j];
				System.out.println("The sum of the row " + i + "is " + sum);
			}
		}
	}
}

