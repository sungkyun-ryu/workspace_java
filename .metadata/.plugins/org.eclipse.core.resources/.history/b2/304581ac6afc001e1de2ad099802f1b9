package QUIZ;

import java.util.Scanner;

public class Quiz1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc= new Scanner(System.in);
		System.out.println("Give me your first chosen number");
		int i= sc.nextInt(); 
		System.out.println("And your second chosen one?");
		int j= sc.nextInt(); 
		
		System.out.println("i multiplies j= " + (i*j));
		System.out.println("i divided by j= " + (i/j));
		System.out.println("The remainder= " + (i%j));
	}

}
