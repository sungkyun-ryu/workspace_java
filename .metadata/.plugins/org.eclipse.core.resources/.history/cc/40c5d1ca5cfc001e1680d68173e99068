package ch3;

public class Apr17_5 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	
	String nation= "KOR";
	
	switch(nation) {
	case "KOR":
	case "JPN":
	case "CHN":
	{System.out.println("This region is East Asia");
	}
	case "USA":
	case "GBR":
	case "GER":
	{System.out.println("This region is the West");
	}
	default: System.out.println("Which region do you like more?");
	}
	}
}
