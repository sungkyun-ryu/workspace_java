package ch3;

public class Apr17_1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int score= 90;
		int score_1= 59;
		String result= "";
		
		if (score >= 60) {
			result= "Accepted";
		}
		
		System.out.println(result);
		System.out.println(score_1 >= 60 ? "Accepted" : "Rejected");
		
	}

}
