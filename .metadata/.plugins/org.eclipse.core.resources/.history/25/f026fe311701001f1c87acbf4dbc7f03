package assignment;

import java.util.Scanner;

public class PrimeNumber1 {
	
	public void checkpm() {
		Scanner sc= new Scanner(System.in);
		System.out.print("What is your number: ");
		int n = sc.nextInt(); 
		
		for(int i=2; i<n; i++) { 
			if(n%i==0) {
				System.out.println("Not a prime number");
				break;
			} else {
				System.out.println("Hooray! You've got it!");
			}
		}
		
	}
}
